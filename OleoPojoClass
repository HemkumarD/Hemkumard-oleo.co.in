package org.oleopojo;

import org.baseclass.OleoBaseClass;
import org.openqa.selenium.WebElement;
import org.openqa.selenium.support.FindBy;
import org.openqa.selenium.support.PageFactory;

public class OleoPojoClass extends OleoBaseClass {
	public OleoPojoClass() {
		PageFactory.initElements(driver,this);
	}
	@FindBy(xpath = "(//a[text()='Homepage'])[1]")
	private WebElement HomeButton;
	@FindBy(xpath = "//a[text()='Login']")
	private WebElement LoginButton;
	@FindBy(xpath = "//a[text()='Contact Us']")
	private WebElement ContactUs;
	@FindBy(xpath ="//a[text()='Get Started']")
	private WebElement Getstarted;
	@FindBy(xpath = "//a[text()='Devices']")
	private WebElement Devices;
	@FindBy(xpath ="//a[text()='Interfaces']")
	private WebElement Interfaces;
	@FindBy(xpath = "//a[text()='Buffer Stops']")
	private WebElement Bufferstops;
	@FindBy(xpath = "//a[text()='Vehicles']")
	private WebElement Vehicles;
	@FindBy(xpath = "//a[text()='Trains']")
	private WebElement Trains;
	@FindBy(xpath = "//a[text()='Getting Started']")
	private WebElement Gettingstarted;
	@FindBy(xpath = "//a[text()='Create Train Impact Simulation']")
	private WebElement cratetrainimpactsimulation;
	@FindBy(xpath = "//a[text()='Create Buffer Stop Impact Simulation']")
	private WebElement Createbufferstopimpactsimulation;
	@FindBy(xpath = "//a[text()='DigitalTrains']")
	private WebElement Digitaltrains;
	@FindBy(xpath = "(//a[text()='Homepage'])[2]")
	private WebElement Homepage;
	@FindBy(className = "help-button-contents")
	private WebElement HelpButton;
	@FindBy(id = "search_input_react")
	private WebElement Searchbox;
	@FindBy(className = "nav-link")
	private WebElement AboutButton;
	@FindBy(id="menu-item-12")
	private WebElement Features;
	@FindBy(id = "menu-item-14")
	private WebElement Contact;
	@FindBy(id = "menu-item-13")
	private WebElement Login;
	@FindBy(className = "purple")
	private WebElement Getintouch;
	@FindBy(xpath = "//a[text()='Subscribe']")
	private WebElement Subscribe;
	@FindBy(name = "your-name")
	private WebElement Yourname;
	@FindBy(name = "email")
	private WebElement Email;
	@FindBy(name = "phone")
	private WebElement phone;
	@FindBy(name = "query")
	private WebElement Resoneforcontaction;
	@FindBy(name = "message")
	private WebElement Messagebox;
	@FindBy(xpath = "//input[@value='Submit']")
	private WebElement Submitbutton;
	@FindBy(id = "modal-login-email")
	private WebElement Usernamebox;
	@FindBy(id = "modal-login-password")
	private WebElement Passwordbox;
	@FindBy(id = "remember")
	private WebElement Remebermebutton;
	@FindBy(xpath = "//input[@value=\"Submit\"]")
	private WebElement Loginsubmit;
	public WebElement getHomeButton() {
		return HomeButton;
	}
	public WebElement getLoginButton() {
		return LoginButton;
	}
	public WebElement getContactUs() {
		return ContactUs;
	}
	public WebElement getGetstarted() {
		return Getstarted;
	}
	public WebElement getDevices() {
		return Devices;
	}
	public WebElement getInterfaces() {
		return Interfaces;
	}
	public WebElement getBufferstops() {
		return Bufferstops;
	}
	public WebElement getVehicles() {
		return Vehicles;
	}
	public WebElement getTrains() {
		return Trains;
	}
	public WebElement getGettingstarted() {
		return Gettingstarted;
	}
	public WebElement getCratetrainimpactsimulation() {
		return cratetrainimpactsimulation;
	}
	public WebElement getCreatebufferstopimpactsimulation() {
		return Createbufferstopimpactsimulation;
	}
	public WebElement getDigitaltrains() {
		return Digitaltrains;
	}
	public WebElement getHomepage() {
		return Homepage;
	}
	public WebElement getHelpButton() {
		return HelpButton;
	}
	public WebElement getSearchbox() {
		return Searchbox;
	}
	public WebElement getAboutButton() {
		return AboutButton;
	}
	public WebElement getFeatures() {
		return Features;
	}
	public WebElement getContact() {
		return Contact;
	}
	public WebElement getLogin() {
		return Login;
	}
	public WebElement getGetintouch() {
		return Getintouch;
	}
	public WebElement getSubscribe() {
		return Subscribe;
	}
	public WebElement getYourname() {
		return Yourname;
	}
	public WebElement getEmail() {
		return Email;
	}
	public WebElement getPhone() {
		return phone;
	}
	public WebElement getResoneforcontaction() {
		return Resoneforcontaction;
	}
	public WebElement getMessagebox() {
		return Messagebox;
	}
	public WebElement getSubmitbutton() {
		return Submitbutton;
	}
	public WebElement getUsernamebox() {
		return Usernamebox;
	}
	public WebElement getPasswordbox() {
		return Passwordbox;
	}
	public WebElement getRemebermebutton() {
		return Remebermebutton;
	}
	public WebElement getLoginsubmit() {
		return Loginsubmit;
	}
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	

}
